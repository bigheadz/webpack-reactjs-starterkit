{
  "presets": [
    "react",
    "stage-2",
    "es2015"
  ],
  "plugins": [
    "transform-class-properties",
    "add-module-exports",
    "transform-runtime"
  ],
  "env": {
    "test": {
      "plugins": [
        "istanbul"
      ]
    },
    "production": {
      "plugins": [
        [
          "babel-plugin-transform-require-ignore",
          {
            "extensions": [
              ".less"
            ]
          }
        ]
      ]
    }
  }
}
